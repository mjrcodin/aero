{
  "name": "uss-aeronyde-server",
  "version": "1.0.0",
  "engines": {
    "npm": "6.1.0",
    "node": "v8.9.4"
  },
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm install && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "pretest": "node ./test/example.js -u",
    "test": "node_modules/mocha/bin/mocha ./test",
    "geo:nsurf": "node mapbox/upload.js NSUFR",
    "geo:uasfm": "node mapbox/upload.js UASFM",
    "geo:uasfm2": "node mapbox/upload.js UASFM2",
    "geo:airports": "node mapbox/upload.js Airports",
    "geo:stadiums": "node mapbox/upload.js Stadiums",
    "geo:classairspace": "node mapbox/upload.js ClassAirspace",
    "geo:status": "node mapbox/status",
    "docs": "node_modules/documentation/bin/documentation.js build api/*/** -f html -o docs"
  },
  "prepush": {
    "run": "test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "documentation": "^8.1.0",
    "dotenv": "^6.0.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nock": "^9.4.3",
    "nodemon": "^1.11.0",
    "pre-push": "^0.1.1"
  },
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-mysql-session": "^2.0.0",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "mysql2": "^1.6.0",
    "node-localstorage": "^1.3.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react": "^16.4.1",
    "react-map-gl": "^3.3.2",
    "spectre.css": "^0.5.3",
    "yarn": "^1.9.2",
    "rotating-file-stream": "^1.3.7",
    "sequelize": "^4.38.0",
    "twilio": "^3.18.0"
  }
}
