{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "linebreak-style": 0,
    "class-methods-use-this": 0,
    "import/no-named-as-default": 0,
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-unused-vars": 1,
    "comma-dangle": [2, "never"],
    "react/jsx-one-expression-per-line": 0,
    "no-return-assign": 1,
    "prefer-template": 0,
    "arrow-parens": 0
  }
}